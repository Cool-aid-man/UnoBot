{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"newline-after-var": "warn",
		"no-unused-vars": "warn",
		"no-mixed-spaces-and-tabs": "off",
		"no-inner-declarations": "off",
		"no-async-promise-executor": "off"
	}
}
